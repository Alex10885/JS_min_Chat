    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^30.1.3",
    "mongodb-memory-server": "^10.2.0",
    "supertest": "^7.1.4"
  }
