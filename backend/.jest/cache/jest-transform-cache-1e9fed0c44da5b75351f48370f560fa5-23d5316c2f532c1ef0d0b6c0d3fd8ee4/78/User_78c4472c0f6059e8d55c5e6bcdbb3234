// Increment failed login attempts
/* istanbul ignore next */
cov_1055pu2obn().s[93]++;
userSchema.methods.incFailedAttempts = function () {
  /* istanbul ignore next */
  cov_1055pu2obn().f[20]++;
  cov_1055pu2obn().s[94]++;
  this.failedLoginAttempts += 1;
  /* istanbul ignore next */
  cov_1055pu2obn().s[95]++;
  this.lastFailedAttempt = new Date();

  // Lock account after 3 failed attempts
  /* istanbul ignore next */
  cov_1055pu2obn().s[96]++;
  if (this.failedLoginAttempts >= 3) {
    /* istanbul ignore next */
    cov_1055pu2obn().b[17][0]++;
    cov_1055pu2obn().s[97]++;
    this.accountLockedUntil = new Date(Date.now() + 10 * 60 * 1000); // 10 minutes lock
    /* istanbul ignore next */
    cov_1055pu2obn().s[98]++;
    this.captchaRequired = true; // Require CAPTCHA when unlocked
  } else
  /* istanbul ignore next */
  {
    cov_1055pu2obn().b[17][1]++;
  }

  // Require CAPTCHA after 2 failed attempts
  cov_1055pu2obn().s[99]++;
  if (this.failedLoginAttempts >= 2) {
    /* istanbul ignore next */
    cov_1055pu2obn().b[18][0]++;
    cov_1055pu2obn().s[100]++;
    this.captchaRequired = true;
  } else
  /* istanbul ignore next */
  {
    cov_1055pu2obn().b[18][1]++;
  }
  cov_1055pu2obn().s[101]++;
  return this.save();
};

