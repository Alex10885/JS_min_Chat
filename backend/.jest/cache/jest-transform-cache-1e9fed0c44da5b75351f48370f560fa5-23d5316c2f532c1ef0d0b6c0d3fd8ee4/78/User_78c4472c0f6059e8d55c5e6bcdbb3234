// Enable 2FA
/* istanbul ignore next */
cov_1055pu2obn().s[120]++;
userSchema.methods.enable2FA = function (method =
/* istanbul ignore next */
(cov_1055pu2obn().b[19][0]++, 'TOTP')) {
  /* istanbul ignore next */
  cov_1055pu2obn().f[27]++;
  cov_1055pu2obn().s[121]++;
  if (!this.twoFactorSecret) {
    /* istanbul ignore next */
    cov_1055pu2obn().b[20][0]++;
    cov_1055pu2obn().s[122]++;
    throw new Error('2FA secret not generated');
  } else
  /* istanbul ignore next */
  {
    cov_1055pu2obn().b[20][1]++;
  }
  cov_1055pu2obn().s[123]++;
  this.twoFactorEnabled = true;
  /* istanbul ignore next */
  cov_1055pu2obn().s[124]++;
  this.twoFactorMethod = method;

  // Generate backup codes (10 codes)
  /* istanbul ignore next */
  cov_1055pu2obn().s[125]++;
  this.backupCodes = [];
  /* istanbul ignore next */
  cov_1055pu2obn().s[126]++;
  for (let i =
  /* istanbul ignore next */
  (cov_1055pu2obn().s[127]++, 0); i < 10; i++) {
    const code =
    /* istanbul ignore next */
    (cov_1055pu2obn().s[128]++, crypto.randomBytes(4).toString('hex').toUpperCase());
    /* istanbul ignore next */
    cov_1055pu2obn().s[129]++;
    this.backupCodes.push(code);
  }
  /* istanbul ignore next */
  cov_1055pu2obn().s[130]++;
  return this.save();
};

// Disable 2FA
/* istanbul ignore next */
cov_1055pu2obn().s[131]++;
userSchema.methods.disable2FA = function () {
  /* istanbul ignore next */
  cov_1055pu2obn().f[28]++;
  cov_1055pu2obn().s[132]++;
  this.twoFactorEnabled = false;
  /* istanbul ignore next */
  cov_1055pu2obn().s[133]++;
  this.twoFactorSecret = null;
  /* istanbul ignore next */
  cov_1055pu2obn().s[134]++;
