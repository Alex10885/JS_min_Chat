// Generate moderation token for admin actions
/* istanbul ignore next */
cov_1055pu2obn().s[70]++;
userSchema.methods.generateModerationToken = function () {
  /* istanbul ignore next */
  cov_1055pu2obn().f[14]++;
  const moderationToken =
  /* istanbul ignore next */
  (cov_1055pu2obn().s[71]++, crypto.randomBytes(32).toString('hex'));
  /* istanbul ignore next */
  cov_1055pu2obn().s[72]++;
  this.moderationToken = crypto.createHash('sha256').update(moderationToken).digest('hex');
  /* istanbul ignore next */
  cov_1055pu2obn().s[73]++;
  this.moderationTokenExpires = Date.now() + 3600000; // 1 hour
  /* istanbul ignore next */
  cov_1055pu2obn().s[74]++;
  return this.save().then(() => {
    /* istanbul ignore next */
    cov_1055pu2obn().f[15]++;
    cov_1055pu2obn().s[75]++;
    return moderationToken;
  });
};

// Verify moderation token
/* istanbul ignore next */
cov_1055pu2obn().s[76]++;
userSchema.methods.verifyModerationToken = function (token) {
  /* istanbul ignore next */
  cov_1055pu2obn().f[16]++;
  const hashedToken =
  /* istanbul ignore next */
  (cov_1055pu2obn().s[77]++, crypto.createHash('sha256').update(token).digest('hex'));
  /* istanbul ignore next */
  cov_1055pu2obn().s[78]++;
  if (hashedToken !== this.moderationToken) {
    /* istanbul ignore next */
    cov_1055pu2obn().b[13][0]++;
    cov_1055pu2obn().s[79]++;
    throw new Error('Invalid moderation token');
  } else
  /* istanbul ignore next */
  {
    cov_1055pu2obn().b[13][1]++;
  }
  cov_1055pu2obn().s[80]++;
  if (Date.now() > this.moderationTokenExpires) {
    /* istanbul ignore next */
    cov_1055pu2obn().b[14][0]++;
    cov_1055pu2obn().s[81]++;
    throw new Error('Moderation token expired');
  } else
  /* istanbul ignore next */
  {
    cov_1055pu2obn().b[14][1]++;
  }
  cov_1055pu2obn().s[82]++;
  this.moderationToken = null;
  /* istanbul ignore next */
  cov_1055pu2obn().s[83]++;
  this.moderationTokenExpires = null;
  /* istanbul ignore next */
  cov_1055pu2obn().s[84]++;
  return this.save();
};

