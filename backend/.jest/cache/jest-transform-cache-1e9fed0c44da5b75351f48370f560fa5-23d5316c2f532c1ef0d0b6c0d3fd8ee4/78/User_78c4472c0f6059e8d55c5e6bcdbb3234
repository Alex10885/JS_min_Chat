// Verify 2FA code
/* istanbul ignore next */
cov_1055pu2obn().s[136]++;
userSchema.methods.verify2FACode = function (code, useBackup =
/* istanbul ignore next */
(cov_1055pu2obn().b[21][0]++, false)) {
  /* istanbul ignore next */
  cov_1055pu2obn().f[29]++;
  cov_1055pu2obn().s[137]++;
  if (
  /* istanbul ignore next */
  (cov_1055pu2obn().b[23][0]++, !this.twoFactorEnabled) ||
  /* istanbul ignore next */
  (cov_1055pu2obn().b[23][1]++, !this.twoFactorSecret)) {
    /* istanbul ignore next */
    cov_1055pu2obn().b[22][0]++;
    cov_1055pu2obn().s[138]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_1055pu2obn().b[22][1]++;
  }

  // Check if using backup code
  cov_1055pu2obn().s[139]++;
  if (
  /* istanbul ignore next */
  (cov_1055pu2obn().b[25][0]++, useBackup) &&
  /* istanbul ignore next */
  (cov_1055pu2obn().b[25][1]++, this.backupCodes.includes(code))) {
    /* istanbul ignore next */
    cov_1055pu2obn().b[24][0]++;
    cov_1055pu2obn().s[140]++;
    // Remove used backup code
    this.backupCodes = this.backupCodes.filter(bc => {
      /* istanbul ignore next */
      cov_1055pu2obn().f[30]++;
      cov_1055pu2obn().s[141]++;
      return bc !== code;
    });
    /* istanbul ignore next */
    cov_1055pu2obn().s[142]++;
    return this.save().then(() => {
      /* istanbul ignore next */
      cov_1055pu2obn().f[31]++;
      cov_1055pu2obn().s[143]++;
      return true;
    });
  } else
  /* istanbul ignore next */
  {
    cov_1055pu2obn().b[24][1]++;
  }
  cov_1055pu2obn().s[144]++;
  if (useBackup) {
    /* istanbul ignore next */
    cov_1055pu2obn().b[26][0]++;
    cov_1055pu2obn().s[145]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_1055pu2obn().b[26][1]++;
  }

  // Verify TOTP
  const speakeasy =
  /* istanbul ignore next */
  (cov_1055pu2obn().s[146]++, require('speakeasy'));
  /* istanbul ignore next */
  cov_1055pu2obn().s[147]++;
  return speakeasy.totp.verify({
    secret: this.twoFactorSecret,
    encoding: 'base32',
    token: code,
    window: 2 // Allow time window for clock skew
  });
};

