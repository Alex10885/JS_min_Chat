  async unlockAccount(req, res) {
    /* istanbul ignore next */
    cov_1ub814s3ey().f[15]++;
    cov_1ub814s3ey().s[125]++;
    try {
      const {
        identifier,
        captchaToken
      } =
      /* istanbul ignore next */
      (cov_1ub814s3ey().s[126]++, req.body);
      const User =
      /* istanbul ignore next */
      (cov_1ub814s3ey().s[127]++, require('../models/User'));
      const user =
      /* istanbul ignore next */
      (cov_1ub814s3ey().s[128]++, await User.findOne({
        $or: [{
          nickname: identifier
        }, {
          email: identifier
        }]
      }));
      /* istanbul ignore next */
      cov_1ub814s3ey().s[129]++;
      if (!user) {
        /* istanbul ignore next */
        cov_1ub814s3ey().b[21][0]++;
        cov_1ub814s3ey().s[130]++;
        return res.status(404).json({
          error: 'User not found'
        });
      } else
      /* istanbul ignore next */
      {
        cov_1ub814s3ey().b[21][1]++;
      }

      // Validate CAPTCHA token in production
      // For now, accept any non-empty token as valid
      cov_1ub814s3ey().s[131]++;
      if (
      /* istanbul ignore next */
      (cov_1ub814s3ey().b[23][0]++, !captchaToken) ||
      /* istanbul ignore next */
      (cov_1ub814s3ey().b[23][1]++, captchaToken.trim().length === 0)) {
        /* istanbul ignore next */
        cov_1ub814s3ey().b[22][0]++;
        cov_1ub814s3ey().s[132]++;
        return res.status(400).json({
          error: 'Valid CAPTCHA token is required'
        });
      } else
      /* istanbul ignore next */
      {
        cov_1ub814s3ey().b[22][1]++;
      }

      // Temporarily reduce failed attempts and unlock account
      cov_1ub814s3ey().s[133]++;
      user.failedLoginAttempts = Math.max(0, user.failedLoginAttempts - 1);
      /* istanbul ignore next */
      cov_1ub814s3ey().s[134]++;
      if (user.failedLoginAttempts < 2) {
        /* istanbul ignore next */
        cov_1ub814s3ey().b[24][0]++;
        cov_1ub814s3ey().s[135]++;
        user.captchaRequired = false;
      } else
      /* istanbul ignore next */
      {
        cov_1ub814s3ey().b[24][1]++;
      }
      cov_1ub814s3ey().s[136]++;
      await user.save();

      // Log security event
      /* istanbul ignore next */
      cov_1ub814s3ey().s[137]++;
      this.logger.info('Account unlocked via CAPTCHA', {
        userId: user._id,
        nickname: user.nickname,
        remainingAttempts: user.failedLoginAttempts,
        captchaRequired: user.captchaRequired
      });
      /* istanbul ignore next */
      cov_1ub814s3ey().s[138]++;
      res.json({
        success: true,
        message: 'Account unlocked successfully',
        captchaRequired: user.captchaRequired
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_1ub814s3ey().s[139]++;
      this.logger.error('Unlock account error:', error);
      /* istanbul ignore next */
      cov_1ub814s3ey().s[140]++;
      res.status(500).json({
        error: 'Server error during unlock',
        code: 'UNLOCK_ERROR'
      });
    }
  }
