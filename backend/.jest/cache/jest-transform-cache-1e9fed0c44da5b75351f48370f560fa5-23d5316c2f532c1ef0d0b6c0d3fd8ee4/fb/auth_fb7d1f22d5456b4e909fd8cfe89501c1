const authenticateSession = async (req, res, next) => {
  /* istanbul ignore next */
  cov_24b6m5euke().f[0]++;
  cov_24b6m5euke().s[6]++;
  try {
    /* istanbul ignore next */
    cov_24b6m5euke().s[7]++;
    console.log('🔐 Session authentication middleware called:', {
      url: req.url,
      method: req.method,
      sessionId: req.sessionID,
      sessionExists: !!req.session,
      sessionData: req.session ?
      /* istanbul ignore next */
      (cov_24b6m5euke().b[0][0]++, {
        authenticated: req.session.authenticated,
        userId: req.session.userId,
        nickname: req.session.nickname,
        role: req.session.role,
        loginTime: req.session.loginTime,
        csrfToken: req.session.csrfToken?.substring(0, 8) + '...'
      }) :
      /* istanbul ignore next */
      (cov_24b6m5euke().b[0][1]++, null)
    });

    // Check if session exists and has authenticated user
    /* istanbul ignore next */
    cov_24b6m5euke().s[8]++;
    if (
    /* istanbul ignore next */
    (cov_24b6m5euke().b[2][0]++, req.session) &&
    /* istanbul ignore next */
    (cov_24b6m5euke().b[2][1]++, req.session.authenticated) &&
    /* istanbul ignore next */
    (cov_24b6m5euke().b[2][2]++, req.session.userId)) {
      /* istanbul ignore next */
      cov_24b6m5euke().b[1][0]++;
      cov_24b6m5euke().s[9]++;
      console.log('🎯 Found authenticated session for userId:', req.session.userId);
      /* istanbul ignore next */
      cov_24b6m5euke().s[10]++;
      console.log('🔓 Session fingerprint check:', {
        sid: req.sessionID,
        csrfToken: req.session.csrfToken?.substring(0, 4) + '...',
        userAgent: req.session.userAgent?.substring(0, 20) + '...',
        loginTime: req.session.loginTime
      });
      const user =
      /* istanbul ignore next */
      (cov_24b6m5euke().s[11]++, await User.findById(req.session.userId));
      /* istanbul ignore next */
      cov_24b6m5euke().s[12]++;
      if (user) {
        /* istanbul ignore next */
        cov_24b6m5euke().b[3][0]++;
        cov_24b6m5euke().s[13]++;
        console.log('✅ Session user found in DB:', {
          nickname: user.nickname,
          id: user._id,
          status: user.status,
          sessionValid: true
        });
        /* istanbul ignore next */
        cov_24b6m5euke().s[14]++;
        req.sessionUser = user; // Store in req.sessionUser to avoid conflict with JWT req.user

        // Update session fingerprint if needed
        /* istanbul ignore next */
        cov_24b6m5euke().s[15]++;
        if (
        /* istanbul ignore next */
        (cov_24b6m5euke().b[5][0]++, req.session.csrfToken) &&
        /* istanbul ignore next */
        (cov_24b6m5euke().b[5][1]++, req.session.userAgent)) {
          /* istanbul ignore next */
          cov_24b6m5euke().b[4][0]++;
          cov_24b6m5euke().s[16]++;
          req.session.lastSessionCheck = new Date().toISOString();
          /* istanbul ignore next */
          cov_24b6m5euke().s[17]++;
          console.log('🔐 Session fingerprint verified and updated');
        } else
        /* istanbul ignore next */
        {
          cov_24b6m5euke().b[4][1]++;
        }
      } else {
        /* istanbul ignore next */
        cov_24b6m5euke().b[3][1]++;
        cov_24b6m5euke().s[18]++;
        console.log('⚠️ Session user not found in DB, cleaning session:', req.session.userId);
        // Clean invalid session
        /* istanbul ignore next */
        cov_24b6m5euke().s[19]++;
        delete req.session.authenticated;
        /* istanbul ignore next */
        cov_24b6m5euke().s[20]++;
        delete req.session.userId;
        /* istanbul ignore next */
        cov_24b6m5euke().s[21]++;
        delete req.session.nickname;
        /* istanbul ignore next */
        cov_24b6m5euke().s[22]++;
        delete req.session.role;
      }
    } else {
      /* istanbul ignore next */
      cov_24b6m5euke().b[1][1]++;
      cov_24b6m5euke().s[23]++;
      console.log('🔍 No authenticated session found or session not initialized', {
        sessionId: req.sessionID,
        session: !!req.session,
        authenticated: req.session?.authenticated,
        userId: req.session?.userId
      });
      /* istanbul ignore next */
      cov_24b6m5euke().s[24]++;
      req.sessionUser = null; // Explicitly set to null when no session
    }
    /* istanbul ignore next */
    cov_24b6m5euke().s[25]++;
    next();
  } catch (error) {
    /* istanbul ignore next */
    cov_24b6m5euke().s[26]++;
    logger.warn('Session authentication error:', {
      error: error.message,
      sessionId: req.sessionID,
      ip: req.ip
    });
    /* istanbul ignore next */
    cov_24b6m5euke().s[27]++;
    req.sessionUser = null; // Set to null on error
    /* istanbul ignore next */
    cov_24b6m5euke().s[28]++;
    next();
  }
};

