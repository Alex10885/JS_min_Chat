const authenticateToken = async (req, res, next) => {
  /* istanbul ignore next */
  cov_24b6m5euke().f[1]++;
  cov_24b6m5euke().s[30]++;
  try {
    /* istanbul ignore next */
    cov_24b6m5euke().s[31]++;
    console.log('🔐 JWT authentication middleware called:', {
      url: req.url,
      method: req.method
    });
    const authHeader =
    /* istanbul ignore next */
    (cov_24b6m5euke().s[32]++, req.headers['authorization']);
    const token =
    /* istanbul ignore next */
    (cov_24b6m5euke().s[33]++,
    /* istanbul ignore next */
    (cov_24b6m5euke().b[6][0]++, authHeader) &&
    /* istanbul ignore next */
    (cov_24b6m5euke().b[6][1]++, authHeader.split(' ')[1])); // Bearer TOKEN
    /* istanbul ignore next */
    cov_24b6m5euke().s[34]++;
    console.log('🔑 Token extraction result:', {
      hasAuthHeader: !!authHeader,
      hasToken: !!token
    });
    /* istanbul ignore next */
    cov_24b6m5euke().s[35]++;
    if (!token) {
      /* istanbul ignore next */
      cov_24b6m5euke().b[7][0]++;
      cov_24b6m5euke().s[36]++;
      console.log('❌ No token provided');
      /* istanbul ignore next */
      cov_24b6m5euke().s[37]++;
      return res.status(401).json({
        error: 'Access token required',
        code: 'NO_TOKEN'
      });
    } else
    /* istanbul ignore next */
    {
      cov_24b6m5euke().b[7][1]++;
    }
    const decoded =
    /* istanbul ignore next */
    (cov_24b6m5euke().s[38]++, jwt.verify(token, config.security.jwtSecret));
    /* istanbul ignore next */
    cov_24b6m5euke().s[39]++;
    console.log('✅ JWT decoded:', {
      userId: decoded.userId,
      nickname: decoded.nickname
    });
    const user =
    /* istanbul ignore next */
    (cov_24b6m5euke().s[40]++, await User.findById(decoded.userId));
    /* istanbul ignore next */
    cov_24b6m5euke().s[41]++;
    if (!user) {
      /* istanbul ignore next */
      cov_24b6m5euke().b[8][0]++;
      cov_24b6m5euke().s[42]++;
      console.log('❌ User not found in DB for JWT userId:', decoded.userId);
      /* istanbul ignore next */
      cov_24b6m5euke().s[43]++;
      return res.status(401).json({
        error: 'User not found',
        code: 'USER_NOT_FOUND'
      });
    } else
    /* istanbul ignore next */
    {
      cov_24b6m5euke().b[8][1]++;
    }
    cov_24b6m5euke().s[44]++;
    req.user = user;
    /* istanbul ignore next */
    cov_24b6m5euke().s[45]++;
    console.log('✅ JWT authentication successful for user:', user.nickname, {
      id: user._id,
      status: user.status
    });
    /* istanbul ignore next */
    cov_24b6m5euke().s[46]++;
    next();
  } catch (error) {
    /* istanbul ignore next */
    cov_24b6m5euke().s[47]++;
    logger.warn('JWT authentication failed:', {
      error: error.message,
      ip: req.ip
    });
    /* istanbul ignore next */
    cov_24b6m5euke().s[48]++;
    if (error.name === 'JsonWebTokenError') {
      /* istanbul ignore next */
      cov_24b6m5euke().b[9][0]++;
      cov_24b6m5euke().s[49]++;
      return res.status(401).json({
        error: 'Invalid token format',
        code: 'INVALID_TOKEN_FORMAT'
      });
    } else
    /* istanbul ignore next */
    {
      cov_24b6m5euke().b[9][1]++;
    }
    cov_24b6m5euke().s[50]++;
    if (error.name === 'TokenExpiredError') {
      /* istanbul ignore next */
      cov_24b6m5euke().b[10][0]++;
      cov_24b6m5euke().s[51]++;
      return res.status(401).json({
        error: 'Token has expired',
        code: 'TOKEN_EXPIRED'
      });
    } else
    /* istanbul ignore next */
    {
      cov_24b6m5euke().b[10][1]++;
    }
    cov_24b6m5euke().s[52]++;
    return res.status(401).json({
      error: 'Token verification failed',
      code: 'TOKEN_VERIFICATION_FAILED'
    });
  }
