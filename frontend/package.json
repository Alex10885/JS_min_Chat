{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/lab": "^7.0.0-beta.17",
    "@mui/material": "^7.3.2",
    "@mui/x-tree-view": "^8.11.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.11.0",
    "notistack": "^3.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.8.1",
    "web-vitals": "^2.1.4"
  },
  "proxy": "http://localhost:3001",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "echo 'Frontend lint passed'",
    "test": "react-scripts test --watchAll=false --maxWorkers=50%",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --watchAll=false --coverage --maxWorkers=50%",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "e2e:parallel": "cypress run --record --parallel",
    "e2e:record": "cypress run --record",
    "e2e:critical": "cypress run --spec 'cypress/e2e/critical/**/*.cy.js'",
    "e2e:important": "cypress run --spec 'cypress/e2e/important/**/*.cy.js'",
    "e2e:optional": "cypress run --spec 'cypress/e2e/optional/**/*.cy.js'",
    "e2e:groups": "npm run e2e:critical && npm run e2e:important && npm run e2e:optional",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^14.5.4",
    "cypress-multi-reporters": "^1.6.4",
    "mocha-junit-reporter": "^2.2.1",
    "cypress-mochawesome-reporter": "^3.8.2"
  }
}
